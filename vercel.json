{
    "version": 2,
    "builds": [
      {
        "src": "astro.config.mjs",
        "use": "@vercel/node"
      },
      {
        "src": "server/index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/server/index.js"
      },
      {
        "src": "/(.*)",
        "dest": "/"
      }
    ]
  }